<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="프로필"
        android:textSize="16sp"
        android:layout_marginTop="70dp"
        android:id="@+id/settingProfile"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="계정"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/settingProfile"
        android:id="@+id/settingAccount"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="알림"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/settingAccount"
        android:id="@+id/settingNotice"/>


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="친구"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/settingNotice"
        android:id="@+id/settingFriend" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="화면"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/settingFriend"
        android:id="@+id/settingDisplay" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="잠금"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/settingDisplay"
        android:id="@+id/settingLock" />

</RelativeLayout>